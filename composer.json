{
    "name": "kode-cms/kode-bundle-core",
    "description": "Symfony 4 Utils: Core library",
    "version": "0.0.1-dev",
    "type": "symfony-bundle",
    "keywords": [
        "kode-cms",
        "symfony",
        "util",
        "mobiledetect"
    ],
    "homepage": "https://github.com/k0d3r1s/kode-bundle-core",
    "license": "MIT",
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/k0d3r1s/kode-bundle-core/issues"
    },
    "require": {
        "php": ">=7.1.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "psr/http-message": "^1.0",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/dependency-injection": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "twig/twig": "^2"
    },
    "autoload": {
        "psr-4": {
            "KodeCms\\KodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeCms\\KodeBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
